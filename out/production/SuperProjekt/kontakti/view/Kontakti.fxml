<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kontakti.controller.KontaktiController">
  <children>
    <GridPane layoutX="0.0" layoutY="0.0" prefHeight="400.0" prefWidth="600.0">
      <children>
        <TableView fx:id="kontaktiTbl" prefHeight="366.0" prefWidth="600.0" GridPane.columnIndex="0" GridPane.rowIndex="0">
          <columns>
            <TableColumn id="imeTblCol" fx:id="imeTblCol" prefWidth="75.0" text="Ime" />
            <TableColumn id="prezimeTblCol" fx:id="prezimeTblCol" prefWidth="75.0" text="Prezime" />
            <TableColumn id="emailTblCol" fx:id="emailTblCol" prefWidth="150.0" resizable="true" text="Email" />
          </columns>
        </TableView>
            <Button mnemonicParsing="false" onAction="#otvoriDodajKorisnika" text="Dodaj kontakt" GridPane.rowIndex="1" />
            <Button mnemonicParsing="false" onAction="#otvoriSveProdukte" text="Otvori produkte" GridPane.rowIndex="2" />
      </children>
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="-1.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
    </GridPane>
  </children>
  <stylesheets>
    <URL value="@kontakti.css" />
  </stylesheets>
</AnchorPane>
